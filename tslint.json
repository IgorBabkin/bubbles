{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single"],
        "ordered-imports": false,
        "jsx-wrap-multiline": false,
        "jsx-no-multiline-js": false,
        "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}
